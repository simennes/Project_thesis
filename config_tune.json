{
  "study_name": "gcn_tuning",
  "n_trials": 40,
  "n_startup_trials": 5,
  "n_warmup_epochs": 5,
  "timeout_seconds": null,
  "base_train": {
    "seed": 42,
    "target_column": "y_adjusted",
    "standardize_features": false,
    "paths": {
      "npz": "Data/gnn/snp_body_mass_ALL.npz",
      "snp_feather": "Data/gnn/SNP/ALL/snp_export_body_mass_ALL_geno.feather",
      "phenotype_csv": "Data/gnn/adjusted_body_mass.csv",
      "grm_rds": "Data/gnn/GRM/GRM_vanraden_HELGELAND.rds",
      "output_dir": "logs/gcn_tuning"
    },
    "graph": {
      "knn_k": 3,
      "weighted_edges": false,
      "symmetrize_mode": "union",
      "self_loops": true,
      "subgraph_size_fraction": 0.5,
      "num_subgraphs_per_epoch": 3
    },
    "model": {
      "hidden_dims": [128, 64],
      "dropout": 0.4,
      "batch_norm": true
    },
    "training": {
      "epochs": 60,
      "optimizer": "adam",
      "lr": 0.001,
      "weight_decay": 1e-5,
      "val_fraction": 0.15,
      "test_fraction": 0.15
    }
  },
  "search_space": {
    "model": {
      "hidden_dims_choices": [[256, 128], [128, 64], [256, 128, 64], [128]],
      "dropout_range": [0.1, 0.6]
    },
    "graph": {
      "knn_k_range": [3, 12],
      "subgraph_fraction_range": [0.2, 0.9],
      "num_subgraphs_range": [1, 8]
    },
    "training": {
      "lr_loguniform": [1e-4, 5e-2],
      "wd_loguniform": [1e-8, 1e-3]
    }
  }
}
