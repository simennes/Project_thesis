{
  "base_train": {
    "paths": {
      "npz": "Data/gnn/snp_thr_tarsus_ALL.npz",
      "snp_feather": "Data/gnn/SNP/ALL/snp_export_body_mass_ALL_geno.feather",
      "phenotype_csv": "Data/gnn/adjusted_thr_tarsus.csv",
      "grm_rds": "Data/gnn/GRM/GRM_vanraden_PLINK.rds",
      "id_column": "ringnr",
      "output_dir": "outputs/nested_cv",
      "output_name": "nested_cv_testing_results"
    },
    "model": {
      "hidden_dims": [128, 64],
      "dropout": 0.2,
      "batch_norm": true
    },
    "training": {
      "lr": 0.001,
      "weight_decay": 1e-5,
      "optimizer": "adam",
      "epochs": 100,
      "loss": "mse"
    },
    "seed": 10,
    "target_column": "y_adjusted",
    "eval_target_column": "y_mean",
    "standardize_features": false
  },

  "cv": {
    "strategy": "leave_island_out",
    "n_splits": 10,
    "inner_splits": 5,
    "shuffle": true,
    "random_state": 10
  },

  "learning_mode": "transductive",

  "search_space": {
    "model": {
      "hidden_dims_choices": [[128, 64], [256, 128], [512, 256], [512, 128], [256, 128, 64], [512, 64]],
      "dropout_range": [0.1, 0.6],
      "batch_norm_choices": [true, false]
    },

    "graph": {
      "graph_on_choices": [true],
      "source_choices": ["grm"],
      "knn_k_range": [1, 7],
      "weighted_edges_choices": [false, true],
      "symmetrize_mode_choices": ["mutual", "union"],

      "graph_on_default": false,
      "source_default": "grm",
      "knn_k_default": 5,
      "weighted_edges_default": true,
      "symmetrize_mode_default": "union"
    },

    "training": {
      "lr_loguniform": [1e-6, 1e-3],
      "wd_loguniform": [1e-7, 1e-3],
      "optimizer_choices": ["adam"],
      "epochs_range": [1, 10],
      "loss_choices": ["mse", "mae"]
    },

    "feature_selection": {
      "use_snp_selection_choices": [true],
      "num_snps_range": [2000, 10000],

      "use_snp_selection_default": false,
      "num_snps_default": 20000
    }
  },

  "n_trials": 3,
  "pruner_warmup_epochs": 2,
  "enable_pruning": false,
  "study_name": "gcnrs_tuning",
  "storage": null,
  "show_progress_bar": true,
  "timeout_seconds": null
}
